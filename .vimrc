set number
set ai
set si
set cindent
set shiftwidth=4
set tabstop=4
set softtabstop=4
set autowrite
set autoread
set ignorecase
set hlsearch
set background=dark
set nocompatible
set fileencodings=utf-8,euc-kr
set bs=indent,eol,start
set history=1000
set ruler
set nobackup
set title
set nowrap
set wmnu

syntax on
colorscheme gruvbox
